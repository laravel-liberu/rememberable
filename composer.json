{
    "name": "laravel-liberu/rememberable",
    "description": "Model caching dependency for Laravel Enso",
    "keywords": [
        "laravel-liberu",
        "rememberable",
        "laravel-cache",
        "model-cache"
    ],
    "homepage": "https://github.com/laravel-liberu/rememberable",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelLiberu\\Rememberable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelLiberu\\Rememberable\\AppServiceProvider"
            ],
            "aliases": {}
        }
    },
    "require-dev": {
        "rector/rector": "^0.17.0"
    }
}
